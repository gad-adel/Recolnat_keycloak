<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
    xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <mirrors>
	  <mirror>
	    <id>mnhn-central</id>
      <name>mnhn-central</name>
	    <url>https://dinsi-nexus.mnhn.cloud/repository/maven-public/</url>
	    <mirrorOf>central</mirrorOf>
	  </mirror>
	</mirrors>
  <profiles>
    <profile>
      <id>mnhn</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>maven-snapshot</id>
          <url>https://dinsi-nexus.mnhn.cloud/repository/maven-snapshots/</url>
        </repository>
        <repository>
          <id>maven-release</id>
          <url>https://dinsi-nexus.mnhn.cloud/repository/maven-releases/</url>
        </repository>
        <repository>
          <id>maven-3rd-party</id>
          <url>https://dinsi-nexus.mnhn.cloud/repository/maven-3rd-party/</url>
        </repository>
      </repositories>
    </profile>
  </profiles>
  <!-- Mnhn Docker Registry -->
  <servers>
    <server>
      <id>docker.mnhn.fr</id>
      <username>${env.RECOLNAT_REGISTRY_USER}</username>
      <password>${env.RECOLNAT_REGISTRY_PASSWORD}</password>
    </server>	 
  </servers>
</settings>
